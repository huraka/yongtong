<script type="text/javascript">
$(document).ready(function(){
	var urla = this.location.href;
	var pos=urla.lastIndexOf("/");
	var pos1=urla.lastIndexOf("/en");
	if(pos1>0){
		var new_url=urla.replace('/en','/cn');
	}else{
		var new_url=urla.replace('/cn','/en');
	}
    var a = urla.substring(pos+1);
    var language = a+'/';
	var lang=$("#switch_lang").attr('tag');
	$("span[id = "+ language +"]").removeClass('top_dh');
	$("span[id = "+ language +"]").addClass('top_dh_on');
	$("#switch_lang").attr("href",new_url);
});
</script>

<script>
    var urla = window.location.href;
    var url = urla.split('/');
    var last = url[4];
    switch(last){
                    case 'products':
            $('#products').addClass('top_dh_on');
            break;
                case 'solutions':
            $('#solutions').addClass('top_dh_on');
            break;
                case 'services':
            $('#services').addClass('top_dh_on');
            break;
                case 'about':
            $('#about').addClass('top_dh_on');
            break;
                case 'contact':
            $('#contact').addClass('top_dh_on');
            break;
                case 'justalk':
            $('#justalk').addClass('top_dh_on');
            break;
                case 'cloud':
            $('#cloud').addClass('top_dh_on');
            break;
                }

</script>		
        
