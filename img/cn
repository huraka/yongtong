	    {
	       if(document.all){
	          document.getElementById(id).click();    
	        }        
	     else
	    {
	      var evt = document.createEvent("MouseEvents");
	       evt.initEvent("click",true,true);
	       document.getElementById(id).dispatchEvent(evt);
	     }  
	   } 
	   function show_tel(){
		$("#tel_detail").show();
	   }
	   function close_tel(){
		$("#tel_detail").hide();
	   }
	   function submit_tel_info(){
		$.post('./email_script/consultation.php',"name="+$("#nick").val()+"&company="+$("#company").val()+"&phone="+$("#phone").val(),function(data){
			if(data.status){
				close_tel();
			}else{
				alert(data.msg);
			}
		},'json')
	   }
